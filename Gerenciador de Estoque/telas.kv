                MDButton:
                    id: 'entrar_button'
                    style: "filled"
                    theme_width: "Custom"
                    height: "40dp"
                    size_hint_x: .8
                    pos_hint: {'center_x': 0.5,'center_y': 0.2}
                    theme_bg_color: 'Custom'
                    md_bg_color: (46/255, 139/255, 87/255, 1)
